{
  "name": "pingo-sidebar",
  "version": "0.0.1",
  "main": "pingo-sidebar.html",
  "authors": [
    "Herb Stahl"
  ],
  "description": "An element that encapsulates iron-ajax and iron-localstorage into one. Use ajax to fetch, then store in localstorage for later retrieval. Cache busting is up to you by what params you pass in.",
  "keywords": [
    "Polymer",
    "web-components"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ghstahl/pingo-sidebar",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "expandjs": "expandjs/ExpandJS#~0.9.9",
    "mat-elements": "expandjs/mat-elements#~0.9.9",
    "polymer": "~1.1.3",
    "xp-elements": "expandjs/xp-elements#~0.9.9"
  }
}
